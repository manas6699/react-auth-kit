{
  "name": "@auth-kit/react-router",
  "version": "3.0.1",
  "description": "React Router Support for React-auth-kit",
  "keywords": [
    "react",
    "router",
    "privateroute",
    "auth"
  ],
  "author": "Arkadip Bhattacharya <hi@arkadip.dev>",
  "homepage": "https://authkit.arkadip.dev/",
  "license": "MIT",
  "source": "/src/index.ts",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "exports": {
    ".": "./index.js",
    "./AuthOutlet": "./AuthOutlet.js",
    "./RequireAuth": "./RequireAuth.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-auth-kit/react-auth-kit.git"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:build": "tsc --project tsconfig.json && babel --config-file ./config/babel.config.js src --out-dir . --extensions \".tsx,.ts,.js,.jsx\"",
    "test": "run-s test:*",
    "test:unit": "jest --config ./config/jest.config.js",
    "lint": "eslint src",
    "test:unit:watch": "jest --watch"
  },
  "bugs": {
    "url": "https://github.com/react-auth-kit/react-auth-kit/issues"
  },
  "peerDependencies": {
    "react": ">=16.8.0 || ^17.x || ^18.x",
    "react-router": ">=6.x"
  },
  "devDependencies": {
    "@types/node": "20.10.8",
    "@types/react": "18.2.47",
    "@types/react-router-dom": "5.3.3",
    "react": "18.2.0",
    "react-router": "6.21.1"
  },
  "gitHead": "5b25edbae186725d50682221b77427200ee89f30",
  "dependencies": {
    "react-auth-kit": "^3.0.1"
  }
}
